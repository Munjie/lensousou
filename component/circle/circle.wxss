.progress_box {
    position: relative;
    z-index: 1;
    margin: 0 auto;
}

.progress_bg {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
}

.progress_canvas {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
}

.progress_info {
    height: 130px;
    font-size: 96rpx;
    line-height: 130px;
    color: #4A4A4A;
}

.progress_text {
    text-align: center;
}