@import "../demo/bank-card/bank-card.wxss";
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    padding-top: 0;
    text-align: center;
}
.canvas_rectangle {
    position: absolute;
}

.beauty-result-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 642rpx;
    padding-top: 32rpx;
}

.title {
    font-size: 36rpx;
    font-family: PingFangSC-Medium;
    color: #4a4a4a;
    line-height: 50rpx;
    padding: 20rpx 0 0px 0;
}

.weui-result_detail_container {
    padding-top: 32rpx;
    padding-bottom: 40rpx;
}

.weui-result_detail {
    color: #666;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    line-height: 34rpx;
    padding-bottom: 12rpx;
}